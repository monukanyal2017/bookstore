{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups":[
    {
      "name":"booksapi",
      "urls":["/api/books"],
      "cacheConfig":{
        "strategy":"freshness",
        "maxSize":20,
        "maxAge":"1hr",
        "timeout":"5s"
      }
    }
  ]
}